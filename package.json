{
  "name": "angular-webpack-workflow",
  "version": "1.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "node_modules/.bin/rimraf dist && node_modules/.bin/webpack --bail --progress --profile",
    "server": "node node_modules/.bin/webpack-dev-server --history-api-fallback --inline --progress",
    "test": "node_modules/.bin/karma start",
    "test-all": "npm run test && npm run test-bdd",
    "test-bdd": "bin/tests",
    "test-watch": "node_modules/.bin/karma start --auto-watch --no-single-run",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldeck/angular-webpack.git"
  },
  "author": "Lachlan Deck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldeck/angular-webpack/issues"
  },
  "homepage": "https://github.com/ldeck/angular-webpack",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-new-router": "^0.5.3",
    "angular-route": "^1.4.3",
    "body-parser": "^1.13.2",
    "bootstrap": "^3.3.5",
    "compression": "^1.5.1",
    "express": "^4.13.1",
    "jquery": "^2.1.4",
    "morgan": "^1.6.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "cucumber": "^0.9.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^1.0.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.2.2",
    "protractor-cucumber-framework": "^0.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
